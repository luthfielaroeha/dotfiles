[core]
  excludesfile = ~/.gitignore

[status]
  showUntrackedFiles = all

[color]
  ui = auto

[alias]
  a    = add
  b    = branch
  bm   = branch --move
  cl   = clone
  cm   = commit -m
  co   = checkout
  cob  = checkout -b
  com  = checkout master
  d    = diff
  dc   = diff --cached
  dm   = diff master..HEAD
  p    = push
  po   = push origin
  pom  = push origin master
  pl   = pull
  rb   = rebase
  rbm  = rebase master
  s    = status
  ss   = status -s
  st   = stash
  stp  = stash pop
