alias reboot='sudo reboot'
alias update='sudo dnf update'

# User specific environment and startup programs
alias cls='clear'
alias :q='exit'
alias ls='ls --color=auto -al'
alias ll='ls -al'
alias l.='ls -d .* --color=auto'
